{
  "name": "unitodo",
  "version": "0.1.6",
  "private": true,
  "scripts": {
    "tauri": "tauri",
    "dev": "tauri dev",
    "dev:release": "tauri dev --release",
    "dev:web": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:web:turbo": "concurrently \"npm run dev:frontend:turbo\" \"npm run dev:backend\"",
    "build": "tauri build",
    "dev:backend": "cargo run --release",
    "dev:frontend": "next dev",
    "dev:frontend:turbo": "next dev --turbo",
    "build:frontend": "next build",
    "build:frontend:turbo": "next build --turbo",
    "start": "next start",
    "generate:grpc": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./app/grpc-generated --grpc_out=grpc_js:./app/grpc-generated --plugin=protoc-gen-grpc=`which grpc_tools_node_protoc_plugin` -I ./src-tauri/proto ./src-tauri/proto/unitodo.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./app/grpc-generated -I ./src-tauri/proto ./src-tauri/proto/unitodo.proto",
    "patch": "cd src-tauri && cargo bump patch && cd .. && git add -A && git commit -m \"chore: cargo bump patch\" && npm version patch",
    "ins": "rm -rf /Applications/Unitodo.app && cp -R src-tauri/target/release/bundle/macos/Unitodo.app /Applications/Unitodo.app"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@grpc/grpc-js": "^1.13.3",
    "@grpc/proto-loader": "^0.7.15",
    "@heroicons/react": "^2.2.0",
    "@lexical/react": "^0.31.0",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.8",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2.2.6",
    "@tauri-apps/plugin-updater": "^2.2.0",
    "@types/lodash": "^4.17.16",
    "axios": "^1.6.5",
    "date-fns": "^4.1.0",
    "google-protobuf": "^3.21.2",
    "lexical": "^0.31.0",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "zustand": "^5.0.4"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.5.0",
    "@types/node": "^20.10.6",
    "@types/react": "^19.1.3",
    "@types/react-dom": "^19.1.3",
    "@types/react-virtualized-auto-sizer": "^1.0.8",
    "@types/react-window": "^1.8.8",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^15.3.2",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "next": "^15.3.2",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^5.3.3"
  },
  "build": {
    "appId": "app.jyu.unitodo",
    "productName": "Unitodo",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "public/icon.icns",
      "target": [
        "dmg"
      ]
    },
    "files": [
      "electron/**/*",
      "out/**/*",
      "app/grpc-generated/**/*"
    ],
    "extraResources": [
      {
        "from": "target/release",
        "to": "rust-backend",
        "filter": [
          "unitodo"
        ]
      }
    ],
    "directories": {
      "buildResources": "resources",
      "output": "dist"
    }
  }
}
